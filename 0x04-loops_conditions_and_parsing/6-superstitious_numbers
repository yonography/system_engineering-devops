#!/usr/bin/env bash
# This script displays numbers from 1 to 20 and adds bad luck messages for 4, 9, and 17
count=1
while [ $count -le 20 ]
do
  echo "$count"
  case $count in
    4)
      echo "bad luck from China"
      ;;
    9)
      echo "bad luck from Japan"
      ;;
    17)
      echo "bad luck from Italy"
      ;;
  esac
  count=$((count + 1))
done
